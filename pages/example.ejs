// Respone Dalam bentuk Text

import fetch from 'node-fetch';

// Definisikan URL endpoint API Anda
const apiUrl = 'https://api.clayzaaubert.my.id/api/ai/simi';

// Gantikan dengan kata dan API key yang sesuai
const text = 'Pertanyaanmu';
const apiKey = 'Your Api key';

// Membuat URL dengan parameter kata dan apikey
const urlWithParams = `${apiUrl}?kata=${encodeURIComponent(text)}&apikey=${encodeURIComponent(apiKey)}`;

// Melakukan request GET ke API
fetch(urlWithParams)
  .then((response) => response.json())
  .then((data) => {
    // Menangani respons dari API
    if (data.status === 'Success') {
      console.log('Respon dari API:', data.message);
    } else {
      console.error('Gagal mendapatkan respons dari API.');
    }
  })
  .catch((error) => {
    // Menangani kesalahan jika terjadi
    console.error('Terjadi kesalahan:', error);
  });
              </textarea>
          </div>


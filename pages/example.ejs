// Respone dalam bentuk Image

import fetch from 'node-fetch';
import { writeFile } from 'fs/promises';

// Definisikan URL endpoint API Anda
const apiUrl = 'https://api.clayzaaubert.my.id/api/ai/stablediffusion?';

// Gantikan dengan kata dan API key yang sesuai
const text = 'text yang ingin di ubah ke IMAGE';
const apiKey = 'Your Api key';

// Membuat URL dengan parameter kata dan apikey
const urlWithParams = `${apiUrl}?q=${encodeURIComponent(text)}&apikey=${encodeURIComponent(apiKey)}`;

// Melakukan request GET ke API
fetch(urlWithParams)
  .then((response) => {
    if (!response.ok) {
      throw new Error('Terjadi kesalahan dalam permintaan API');
    }
    return response.arrayBuffer();
  })
  .then(async (imageBuffer) => {
    // Menyimpan gambar ke file
    await writeFile('output.png', Buffer.from(imageBuffer));
    console.log('Gambar telah disimpan sebagai output.png');
  })
  .catch((error) => {
    // Menangani kesalahan jika terjadi
    console.error('Terjadi kesalahan:', error);
  });
              </textarea>
          </div>

